{
  "name": "rosalana/core",
  "description": "Rosalana core package for Laravel apps",
  "type": "library",
  "keywords": [
    "laravel",
    "package",
    "rosalana",
    "core"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "michaelbany",
      "email": "mobany526@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0",
    "illuminate/console": "^12.0",
    "illuminate/filesystem": "^12.0",
    "symfony/console": "^7.0",
    "laravel/prompts": "^0.3.5",
    "symfony/polyfill-php85": "^1.33",
    "rosalana/configure": "^1.0",
    "ext-redis": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "orchestra/testbench": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Rosalana\\Core\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Rosalana\\Core\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rosalana\\Core\\Providers\\RosalanaCoreServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
